                        	<div class="slds-size--1-of-6" ng-if="!editmodebol">
                            	icon  : 
                         	</div>
                        	<div class="slds-col " ng-if="!editmodebol">
                            	<span class="slds-icon_container ">
                                	<svg aria-hidden="true" class="slds-icon  slds-icon--small slds-icon-text-default">
                                    	<use xlink:href="{{displayurl + iconurl}}"></use>
                                	</svg>
                            	</span>
